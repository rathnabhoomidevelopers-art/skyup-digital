{
  "redirects": [
    {
      "source": "https://skyupdigitalsolutions.com/:path*",
      "destination": "https://www.skyupdigitalsolutions.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://skyup-backend.vercel.app/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Robots-Tag", "value": "" }
      ]
    },
    {
      "source": "/contactus",
      "headers": [
        { "key": "X-Robots-Tag", "value": "" }
      ]
    }
  ]
}
