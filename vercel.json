{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "skyupdigitalsolutions.com" }],
      "destination": "https://www.skyupdigitalsolutions.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://skyup-backend.vercel.app/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [{ "key": "Content-Type", "value": "application/xml" }]
    }
  ]
}
