{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.skyupdigitalsolutions.com",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@sparticuz/chromium": "^122.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "formik": "^2.4.9",
    "framer-motion": "^12.23.24",
    "gsap": "^3.14.2",
    "lucide-react": "^0.555.0",
    "puppeteer-core": "^22.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.2",
    "react-scripts": "5.0.1",
    "swiper": "^12.0.3",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node scripts/generate-sitemap.js || echo 'Sitemap generation skipped'",
    "build": "react-scripts build && react-snap",
    "postbuild": "node scripts/verify-meta-tags.js || echo 'Meta verification skipped'",
    "build:no-snap": "react-scripts build",
    "verify-meta": "node scripts/verify-meta-tags.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
"reactSnap": {
  "include": [
    "/",
    "/aboutus",
    "/service",
    "/contactus",
    "/blogs",
    "/careers",
    "/privacypolicy",
    "/termscondition",
    "/thankyou"
  ],
  "source": "build",
  "puppeteerArgs": [
    "--no-sandbox",
    "--disable-setuid-sandbox",
    "--disable-dev-shm-usage",
    "--disable-accelerated-2d-canvas",
    "--disable-gpu"
  ],
  "puppeteerExecutablePath": null,
  "skipThirdPartyRequests": true,
  "cacheAjaxRequests": false,
  "preloadImages": false,
  "inlineCss": false,
  "minifyHtml": {
    "collapseWhitespace": false,
    "removeComments": false,
    "minifyCSS": false,
    "minifyJS": false
  },
  "crawl": true,
  "waitFor": 3000,
  "fixWebpackChunksIssue": "CRA2",
  "asyncScriptTags": false,
  "removeBlobs": false,
  "removeStyleTags": false,
  "preconnectThirdParty": false
},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.18"
  }
}