{
  "overrides": {
    "puppeteer": "npm:puppeteer-core@19.11.1"
  },
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.skyupdigitalsolutions.com",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bootstrap": "^5.3.8",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "formik": "^2.4.9",
    "framer-motion": "^12.23.24",
    "gsap": "^3.14.2",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.555.0",
    "pdfmake": "^0.3.3",
    "puppeteer-core": "^19.11.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^6.30.2",
    "react-to-print": "^3.2.0",
    "swiper": "^12.0.3",
    "vike": "^0.4.253",
    "vike-react": "^0.6.20",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.1"
  },
  "scripts": {
    "start": "vite",
    "prebuild": "node scripts/generate-sitemap.js || echo Sitemap generation skipped",
    "build": "vite build && node scripts/run-react-snap.js",
    "postbuild": "echo Build completed successfully!",
    "build:no-snap": "vite build",
    "verify-meta": "node scripts/verify-meta-tags.js",
    "test": "vitest",
    "preview": "vite preview"
  },
  "reactSnap": {
    "include": [
      "/",
      "/aboutus",
      "/service",
      "/contactus",
      "/blogs",
      "/careers",
      "/privacypolicy",
      "/termscondition",
      "/thankyou"
    ],
    "source": "build",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--disable-gpu"
    ],
    "puppeteerExecutablePath": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "preloadImages": false,
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false,
      "minifyCSS": false,
      "minifyJS": false
    },
    "crawl": true,
    "waitFor": 3000,
    "fixWebpackChunksIssue": "CRA2",
    "asyncScriptTags": false,
    "removeBlobs": false,
    "removeStyleTags": false,
    "preconnectThirdParty": false
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.18",
    "vite": "^7.3.1"
  }
}
